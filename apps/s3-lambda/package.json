{
    "name": "@app/s3-lambda",
    "version": "0.0.1",
    "private": true,
    "nx": {
        "targets": {
            "build": {
                "executor": "@nx/webpack:webpack",
                "defaultConfiguration": "development",
                "options": {
                    "main": "apps/s3-lambda/src/main.ts",
                    "target": "node",
                    "compiler": "tsc",
                    "tsConfig": "apps/s3-lambda/tsconfig.app.json",
                    "outputPath": "dist/apps/s3-lambda",
                    "deleteOutputPath": true,
                    "webpackConfig": "apps/s3-lambda/webpack.config.js"
                },
                "configurations": {
                    "development": {
                        "watch": true,
                        "watchOptions": {
                            "poll": 1000
                        },
                        "sourceMap": true,
                        "optimization": false,
                        "outputHashing": "none"
                    },
                    "production": {
                        "generatePackageJson": true,
                        "sourceMap": false,
                        "optimization": true,
                        "outputHashing": "all"
                    }
                }
            },
            "dev": {
                "executor": "@nx/js:node",
                "options": {
                    "buildTarget": "s3-lambda:build:development",
                    "watch": true
                }
            }
        }
    }
}